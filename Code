import java.io.*;
class Employee implements Serializable
{
int id;
String name;
double payrate;
Employee(int id,String name,double payrate)
{
this.id=id;
this.name=name;
this.payrate=payrate;
}
public String toString()
{
return id+name+payrate;
}
public static void main(String[] args) throws IOException
{
Employee e1=new Employee(123,"varun",450.0);

FileOutputStream fout=new FileOutputStream("sai.txt");
ObjectOutputStream oos=new ObjectOutputStream(fout);
oos.writeObject(e1);
oos.flush();
oos.close();
Employee e2;
try
{
FileInputStream fin=new FileInputStream("sai.txt");
ObjectInputStream oin=new ObjectInputStream(fin);
e2=(Employee)oin.readObject();
System.out.println(e2);
oin.close();
}
catch(Exception e)
{
System.out.println("error is in deserialization");
}
}
}
